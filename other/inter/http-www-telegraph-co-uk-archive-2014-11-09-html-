                                var menuSec = "opta-" + $('meta[name="GSASectionUniqueName"]').attr("content");
                                $.cookie('tmg_navPos', menuSec, { path: '/', domain: 'telegraph.co.uk'});
                            };

                            textNode.replaceWith(link);
                        });
            }
